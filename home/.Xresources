! URxvt
URxvt.font: xft:Hack:size=9, xft:Awesome:size=9
URxvt.scrollBar: false
URxvt.letterSpace: -1
!! Borders
URxvtURxvtexternalBorder: 0
URxvtURxvtinternalBorder: 0
URxvtURxvtborderLess: false
! Transparency
URxvt.transparent: false
URxvt.shading: 8
URxvt.perl-ext-common: default,matcher,url-select,keyboard-select,clipboard,font-size
URxvt.colorURL: $808050
!! url-select:
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true
!!keyboard:select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate
!! matcher
URxvt.url-launcher:	/usr/bin/xdg-open
URxvt.matcher.button:	1

! special
URxvt.foreground:   #bbaa99
URxvt.background:   #050902
URxvt.cursorColor:  #746c48

! black
URxvt.color0:       #121110
URxvt.color8:       #544b2e

! red
URxvt.color1:       #98724c
URxvt.color9:       #af652f

! green
URxvt.color2:       #908f32
URxvt.color10:      #c3c13d

! yellow
URxvt.color3:       #aa964c
URxvt.color11:      #c8b55b

! blue
URxvt.color4:       #7b854e
URxvt.color12:      #70a16c

! magenta
URxvt.color5:       #6b5644
URxvt.color13:      #98724c

! cyan
URxvt.color6:       #5c5142
URxvt.color14:      #778725

! white
URxvt.color7:       #c8b55b
URxvt.color15:      #e4dc8c

! ROFI configuration
rofi.lines: 6
rofi.eh: 1
rofi.padding: 10
rofi.fullscreen: false
rofi.bw: 1
rofi.width: 25
rofi.separator-style: solid
rofi.hide-scrollbar: true

!                  'background', 'border', 'separator'
rofi.color-window: argb:99121110, #908570, #908570
! State:           'bg',     'fg',     'bgalt',  'hlbg',   'hlfg'
rofi.color-normal: argb:75121110, #908570, argb:03aa964c, argb:03544b2e, #e4dc8c
rofi.color-urgent: argb:75121110, #908570, argb:bc377872, argb:03544b2e, #e4dc8c
rofi.color-active: argb:75121110, #908570, argb:03aa964c, argb:03544b2e, #e4dc8c
rofi.font: Hack Cantarell 11
